{{#each passports as |passport|}}
  {{log passport}}
  <!-- {{> partial}}
  Don't uncomment yet -->

  <div>
    <h1 data-id="{{passport.id}}"> <small>{{limit passport.title 10}}</small></h1>
    <section data-id="{{passport.id}}"
    <p>
      url/app: {{passport.url}}<br>
      ID: {{passport.id}} <br>
      user: {{passport.email}}<br>
      Password: {{passport.cred}}
    </p>
    <button type="submit" class="btn btn-default">Remove</button>
    </section>

  </div>

{{/each}}
