{{#each passports as |passport|}}
  {{log passport}}
  <!-- {{> partial}}
  Don't uncomment yet -->

  <div>
    <h1 data-id="{{passport.id}}"> <small>Title: {{limit passport.title 10}}</small></h1>
    <section data-id="{{passport.id}}"
    <p>{{#if passport.cred}}
      {{passport.id}}
      {{passport.cred}}
    {{else}}
      No password provided.
    {{/if}}
    </p>
    <button type="submit" class="btn btn-default">Remove</button>
    </section>

  </div>

{{/each}}
